# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
MONGODB_URI=mongodb://localhost:27017/ssl-oj
MONGODB_TEST_URI=mongodb://localhost:27017/ssl-oj-test

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRE=7d
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production
JWT_REFRESH_EXPIRE=30d

# 文件上传配置
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# 判题系统配置
JUDGE_DOCKER_IMAGE=ssl-oj-judge:latest
JUDGE_TIMEOUT=30000
JUDGE_MEMORY_LIMIT=256
JUDGE_QUEUE_NAME=judge-queue

# 邮件配置（可选）
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_FROM=

# 日志配置
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# 安全配置
CORS_ORIGIN=http://localhost:5173
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# 管理员配置
ADMIN_EMAIL=admin@ssl-oj.com
ADMIN_PASSWORD=admin123456

# 学校信息
SCHOOL_NAME=东莞中学松山湖学校（集团）东莞市第十三高级中学
SCHOOL_SHORT_NAME=SSL
SYSTEM_NAME=SSL-OJ